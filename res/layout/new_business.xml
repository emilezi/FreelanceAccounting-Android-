<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView"
        android:layout_width="350dp"
        android:layout_height="65dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:text="Nouveau profil"
        android:textSize="28dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textFirstName"
        android:layout_width="350dp"
        android:layout_height="25dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="90dp"
        android:text="Prénom"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/first_name"
        android:layout_width="350dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="100dp"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textLastName"
        android:layout_width="350dp"
        android:layout_height="25dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="150dp"
        android:text="Nom"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/last_name"
        android:layout_width="350dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="160dp"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textIdentifier"
        android:layout_width="350dp"
        android:layout_height="25dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="210dp"
        android:text="Identifiant"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/identifier"
        android:layout_width="350dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="220dp"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textEmail"
        android:layout_width="350dp"
        android:layout_height="25dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="270dp"
        android:text="Email"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/email"
        android:layout_width="350dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="280dp"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textPhone"
        android:layout_width="350dp"
        android:layout_height="25dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="330dp"
        android:text="Téléphone"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/phone"
        android:layout_width="350dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="340dp"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textSIREN"
        android:layout_width="350dp"
        android:layout_height="25dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="390dp"
        android:text="SIREN"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/SIREN"
        android:layout_width="350dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="400dp"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textSIRET"
        android:layout_width="350dp"
        android:layout_height="25dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="450dp"
        android:text="SIRET"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/SIRET"
        android:layout_width="350dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="460dp"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textPassword"
        android:layout_width="350dp"
        android:layout_height="25dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="510dp"
        android:text="Mot de passe"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/password"
        android:layout_width="350dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="520dp"
        android:inputType="textPassword"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textRepassword"
        android:layout_width="350dp"
        android:layout_height="25dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="570dp"
        android:text="Retaper le mot de passe"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/repassword"
        android:layout_width="350dp"
        android:layout_height="45dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="580dp"
        android:inputType="textPassword"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_new_business"
        android:layout_width="198dp"
        android:layout_height="58dp"
        android:layout_marginStart="160dp"
        android:layout_marginTop="640dp"
        android:layout_marginEnd="160dp"
        android:text="Créer le profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>